{
    "name": "reactive-docs",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "lint": "eslint .",
        "start": "next start",
        "export": "next export && echo \"This file tells github not to use jekyll.\nIt is required because jekyll not resolves underscore folders, like _next\" > out/.nojekyll",
        "push-gh-pages": "node tools/scripts/publish.js",
        "deploy": "npm run build && npm run export && npm run push-gh-pages",
        "generate:page-indexes": "node tools/scripts/indexPages.js"
    },
    "dependencies": {
        "@material-ui/core": "^4.11.0",
        "@mdx-js/loader": "^1.6.18",
        "@mdx-js/react": "^1.6.18",
        "@next/mdx": "^9.5.3",
        "@types/classnames": "^2.2.10",
        "classnames": "^2.2.6",
        "copy-to-clipboard": "^3.3.1",
        "gray-matter": "^4.0.2",
        "imagemin-svgo": "^8.0.0",
        "minimatch": "^3.0.4",
        "morfix": "file:..",
        "next": "^12.1.0",
        "next-optimized-images": "^2.6.2",
        "prism-react-renderer": "^1.1.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-live": "^2.2.2",
        "remark-slug": "^6.0.0",
        "sass": "^1.26.11"
    },
    "devDependencies": {
        "@reactive-tools/eslint-config": "*",
        "@types/ejs": "^3.0.5",
        "@types/node": "^14.11.2",
        "ejs": "^3.1.5",
        "gh-pages": "^3.1.0",
        "react-docgen-typescript": "^1.20.5",
        "typescript": "^4.0.3",
        "yargs": "^16.1.0"
    }
}
