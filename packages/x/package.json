		"test:watch": "jest --passWithNoTests --watch"
	},
	"devDependencies": {
		"@babel/core": "7.19.6",
		"@reactive-forms/core": "workspace:*",
		"@reactive-tools/eslint-config": "workspace:*",
		"@types/jest": "26.0.24",
		"@types/react": "18.0.23",
		"aqu": "0.4.3",
		"jest": "29.2.2",
		"react": "18.2.0",
		"rimraf": "3.0.2",
		"typescript": "4.8.4"
	},
	"peerDependencies": {
		"@reactive-forms/core": "< 1.0.0",
		"react": ">=16"
	},
	"files": [
		"dist"
	],
	"source": "src/index.ts"
